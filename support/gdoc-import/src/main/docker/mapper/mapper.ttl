d2rq:uriPattern "http://publications.europa.eu/resource/authority/data-theme/@@Dataset.tema@@";
d2rq:condition "Dataset.@tema != ''"; # optional
.

# Provinens - ok
map:provinens a d2rq:PropertyBridge;
