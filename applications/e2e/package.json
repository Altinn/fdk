{
  "name": "e2e",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test:browser": "bash -c \"BROWSER=chromium npm t\"",
    "test:skipoutfile": "./wait_for_dependencies.sh && jest",
    "test": "bash -c \" ./wait_for_dependencies.sh && jest > >(tee ./reports/stdout.log) 2> >(tee ./reports/test-report.txt >&2)\"",
    "test:in_container": "BROWSER=google-chrome-unstable HOST_SEARCH=http://nginx-search:8080 npm t",
    "lint": "eslint ./ --ext=.js --fix"
  },
  "lint-staged": {
    "gitDir": "../../",
    "linters": {
      "*.{js}": [
        "lint",
        "git add"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.4.0",
    "delay": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.4.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "json2csv": "^4.3.5",
    "puppeteer": "^1.13.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jestsetup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.js",
      "**/?(*.)(test).js?(x)"
    ],
    "reporters": [
      "default",
      "<rootDir>/lib/arts-reporter"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/fixtures/"
    ]
  }
}
