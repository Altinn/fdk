{
  "name": "e2e",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test:browser": "bash -c \"BROWSER=chromium npm t\"",
    "test:skipoutfile": "./wait_for_dependencies.sh && jest",
    "test": "bash -c \" ./wait_for_dependencies.sh && jest > >(tee ./reports/stdout.log) 2> >(tee ./reports/test-report.txt >&2)\"",
    "test:in_container": "BROWSER=google-chrome-unstable HOST_SEARCH=http://nginx-search:8080 npm t",
    "lint:project": "eslint .",
    "lint:project:fix": "eslint . --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.4.0",
    "delay": "^4.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.4.0",
    "json2csv": "^4.3.5",
    "prettier": "^1.19.1",
    "puppeteer": "^1.13.0"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jestsetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/test/**/*.test.js"
    ],
    "reporters": [
      "default",
      "<rootDir>/src/lib/arts-reporter"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/test/fixtures/"
    ]
  }
}
